@model test.ViewModel.BasicData.VirusVMs.VirusVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:textbox field="Entity.VirusCode" />
<wt:textbox field="Entity.VirusName" />
<wt:combobox field="Entity.VirusType" />
<wt:textbox field="Entity.Remark" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
