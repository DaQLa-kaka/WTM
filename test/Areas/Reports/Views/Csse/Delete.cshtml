@model test.ViewModel.Reports.CsseVMs.CsseVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
	<wt:quote>@Localizer["Sys.DeleteConfirm"]</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.Country" />
<wt:display field="Entity.Province" />
<wt:display field="Entity.Lat" />
<wt:display field="Entity.Long" />
<wt:display field="Entity.Date" />
<wt:display field="Entity.ConfirmedCase" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="@Localizer["Sys.Delete"]"/>
        <wt:closebutton />
    </wt:row>
</wt:form>
